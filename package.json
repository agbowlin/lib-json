{
	"name": "@liquicode/lib-json",
	"version": "0.1.4",
	"description": "A replacement library for JavaScript's standard JSON functions and more",
	"license": "MIT",
	"homepage": "http://lib-json.liquicode.com",
	"repository": {
		"type": "git",
		"url": "https://github.com/agbowlin/lib-json"
	},
	"authors": [
		{
			"name": "Andre' G. Bowlin",
			"email": "agbowlin@gmail.com",
			"homepage": "http://liquicode.com"
		}
	],
	"main": "src/lib-json.js",
	"files": [
		"readme.md",
		"license.md",
		"VERSION",
		"dist/*",
		"docs/*",
		"src/*",
		"tests/*"
	],
	"ignore": [
		".*",
		"~*",
		"bower_components",
		"node_modules",
		"build"
	],
	"keywords": [
		"javascript",
		"js",
		"nodejs",
		"node",
		"json",
		"Traverse",
		"HasPath",
		"FindName",
		"FindValue",
		"GetValue",
		"SetValue",
		"Stringify",
		"Tablify",
		"ToIniText",
		"FromIniText",
		"Parse",
		"Tokenize",
		"BuildObject",
		"Clone"
	],
	"dependencies": {},
	"devDependencies": {
		"@vivaxy/here": "^3.4.1",
		"mocha": "^8.3.0",
		"webpack": "^5.24.3",
		"webpack-cli": "^4.5.0",
		"webpack-node-externals": "^2.5.2"
	},
	"scripts": {
		"run-tests": "mocha -u bdd tests/*.js --timeout 0 --slow 10 --colors",
		"run-docs-web-server": "npx here --directory docs --port 4200 --watch 1 --silent",
		"010-webpack": "bash build/webpack/010-webpack.sh",
		"810-s3-sync-docs": "bash build/s3/810-s3-sync-docs.sh",
		"910-release-version": "node build/custom/910-release-version.js"
	}
}
